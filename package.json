{
  "name": "@ranger/theme-framework",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +"
  },
  "devDependencies": {
    "@ranger/eslint-config": "workspace:^",
    "@ranger/ts-config": "workspace:^",
    "prettier": "^3.1.1",
    "turbo": "^1.12.4"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  }
}

{
  "name": "@ranger-theme/framework",
  "version": "0.1.0",
  "private": true,
  "config": {
    "cz-customizable": {
      "config": "./.cz-config.js"
    }
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "pnpm run clean:turbo && pnpm run clean:node_modules && pnpm run clean:next && pnpm run clean:dist",
    "clean:turbo": "find . -name '.turbo' -type d -prune -exec rm -rf '{}' +",
    "clean:node_modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "clean:next": "find . -name '.next' -type d -prune -exec rm -rf '{}' +",
    "clean:dist": "find . -name 'dist' -type d -prune -exec rm -rf '{}' +",
    "ngrok": "node ./ngrok.js",
    "husky:prepare": "husky install"
  },
  "dependencies": {
    "@mui/material": "^5.5.11",
    "ngrok": "5.0.0-beta.2",
    "only-allow": "^1.2.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@ranger-theme/branchlint-config": "workspace:^",
    "@ranger-theme/commitizen-config": "workspace:^",
    "@ranger-theme/commitlint-config": "workspace:^",
    "@ranger-theme/eslint-config": "workspace:^",
    "@ranger-theme/prettier-config": "workspace:^",
    "@ranger-theme/stylelint-config": "workspace:^",
    "@ranger-theme/ts-config": "workspace:^",
    "eslint": "^8.2.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "turbo": "^1.12.4"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  }
}

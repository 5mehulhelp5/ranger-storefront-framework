{
  "name": "@ranger-theme/vite-config",
  "version": "1.0.3",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist",
    "lib",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
